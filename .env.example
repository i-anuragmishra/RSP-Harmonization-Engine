# RSP Harmonization Engine Configuration
# Copy this file to .env and fill in your values

# Anthropic API Key (required for LLM extraction)
ANTHROPIC_API_KEY=your_api_key_here

# Model Configuration
# Default model for extraction tasks
LLM_MODEL=claude-sonnet-4-20250514

# Rate Limiting
# Maximum requests per minute to the API
RATE_LIMIT_RPM=50

# Paths (optional - defaults are relative to project root)
# DATA_DIR=./data
# OUTPUT_DIR=./outputs

# Logging
LOG_LEVEL=INFO

# Dashboard Configuration
DASHBOARD_PORT=8501
DASHBOARD_HOST=localhost
